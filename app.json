{
  "expo": {
    "name": "Sonny-todo-app",
    "slug": "sonny-todo-app",
    "version": "1.0.2",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/todo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/todo.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/images/todo.png"
    },
    "description": "A Todo App built with react native and expo",
    "githubUrl": "https://github.com/Sonnysam/react-native-todo-list-app"
  }
}
